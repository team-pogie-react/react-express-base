    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "grommet": "^2.7.8",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "npx": "^10.2.0",
    "pre-push": "^0.1.1",
    "prop-types": "^15.7.2",
