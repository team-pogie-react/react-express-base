{
  "name": "base-react-express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npx nodemon --require @babel/register src/server.js",
    "serve": "node ./dist/server.js",
    "watch": "npx babel ./src -d ./dist -w --copy-files",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "build": "rimraf ./dist && npx babel ./src -d ./dist --copy-files",
    "test": "NODE_ENV=test npx mocha --opts ./tests/mocha.opts --exit",
    "test:all": "npm run test -- --recursive ./tests/",
    "test:unit": "npm run test -- --recursive ./tests/unit",
    "test:integration": "npm run test -- --recursive ./tests/integration",
    "webpack": "webpack --mode production"
  },
  "pre-push": [
    "lint"
  ],
  "author": "Edward",
  "license": "ISC",
  "devDependencies": {
