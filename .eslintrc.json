{
  "extends": [
    "eslint:recommended",
    "airbnb-base/legacy",
    "prettier",
    "plugin:security/recommended",
    "plugin:react/recommended",
    "jest-enzyme"],
  "rules": {
    "newline-before-return": ["error"],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "linebreak-style": 0,
    "validateLineBreaks": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint"
}
